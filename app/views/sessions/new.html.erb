<h1>ログイン画面</h1>
<%= form_with url: login_path, local: true do |f| %>
  <div class="field">
    <%= f.label :email %>
    <%= f.text_field :email %> 
  </div>

  <div class="field">
    <%= f.label :password %>
    <%= f.password_field :password %> 
  </div>

  <div>
    <%= f.submit "login !! " %>
  </div>
<% end %>

<p>アカウントがない場合 <%= link_to "sign up!", new_user_path %> </p>